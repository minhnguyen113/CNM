{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-required-children": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select",
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent",
          "color-adjust",
          "backdrop-filter"
        ]
      }
    ],
    "create-element-svg": "off"
  }
}